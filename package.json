{
  "name": "n8n-nodes-pollinations-ai",
  "version": "1.4.1",
  "description": "n8n community node for Pollinations AI - image, text, speech and music generation",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "pollinations",
    "ai",
    "image-generation",
    "flux",
    "chat-model",
    "llm",
    "langchain",
    "audio",
    "tts",
    "speech",
    "music"
  ],
  "license": "MIT",
  "homepage": "https://github.com/new-xmon-df/n8n-nodes-pollinations-ai",
  "author": {
    "name": "Juanjo GarcÃ­a",
    "email": "juanjogarciae@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/new-xmon-df/n8n-nodes-pollinations-ai.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "index.js",
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/PollinationsApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Pollinations/Pollinations.node.js",
      "dist/nodes/Pollinations/PollinationsChatModel.node.js"
    ],
    "strict": false
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "n8n-node build",
    "dev": "n8n-node dev",
    "lint": "n8n-node lint",
    "lintfix": "n8n-node lint --fix",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@n8n/node-cli": "^0.17.0",
    "@types/node": "^20.10.0",
    "eslint": "^9.32.0",
    "n8n-workflow": "^1.23.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "@langchain/core": "^1.1.12",
    "@langchain/openai": "^1.2.1"
  }
}
